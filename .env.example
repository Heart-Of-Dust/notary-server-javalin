# 服务器配置
NOTARY_PORT=8080
NOTARY_ENV=production

# 数据库配置
DB_URL=jdbc:postgresql://postgres:5432/notary_db
DB_USER=notary_admin
DB_PASSWORD=your_secure_password_here

# Redis配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# 加密配置
MASTER_KEY_ALIAS=notary-master-key
HSM_PROVIDER=aws-kms

# 时间验证配置
TSA_TOLERANCE_SECONDS=90
TSA_MAX_AGE_SECONDS=300

# 日志配置
LOG_LEVEL=INFO
LOG_FORMAT=json